<!DOCTYPE html>
<html>

<head>
  <%= csrf_meta_tag %>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" nanrmpme="viewport">
    <title></title>
    <!-- Favicon-->
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
</head>

<body class="theme-blue">
    <!-- Page Loader -->
    <div class="page-loader-wrapper no-print">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Please wait...</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Search Bar -->
    <div class="search-bar no-print">
        <div class="search-icon">
            <i class="material-icons">search</i>
        </div>
        <input type="text" placeholder="START TYPING...">
        <div class="close-search">
            <i class="material-icons">close</i>
        </div>
    </div>
    <!-- #END# Search Bar -->
    <!-- Top Bar -->
    <nav class="navbar no-print">
        <div class="container-fluid">
            <div class="navbar-header">
                <a href="javascript:void(0);" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false"></a>
                <a href="javascript:void(0);" class="bars"></a>
              <h4><%= image_tag 'fdre.png', size: '40x40' %> FMOH - Annual Review Meeting</h4>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <!-- Call Search -->
                    <li><a href="javascript:void(0);" class="js-search" data-close="true"><i class="material-icons">search</i></a></li>
                    <!-- #END# Call Search -->


                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button">
                          <span style="vertical-align: super"><%= current_user.email rescue nil %></span> <i class="material-icons">keyboard_arrow_down</i>
                        </a>
                      <ul class="dropdown-menu">
                        <li>
                          <%= link_to edit_user_registration_path do %>
                              <i class="material-icons">lock</i>
                              <span>Change Password</span>
                          <% end %>
                        </li>
                        <li role="seperator" class="divider"></li>
                        <li>
                          <%= link_to destroy_user_session_path do %>
                              <i class="material-icons">input</i>
                              <span>Sign Out</span>
                          <% end %>
                        </li>
                        </li>
                      </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- #Top Bar -->
    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar no-print">
            <!-- Menu -->
            <%= render partial: 'menus/admin' %>
            <%= render partial: 'menus/officer' %>
            <!-- #Menu -->
            <!-- Footer -->
            <div class="legal">
                <div class="copyright">
                    &copy; <%= Date.today.year %> FMOH - ARM</a>.
                </div>
            </div>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->
    </section>
    <section class="content">
      <div class="container-fluid">
        <div class="row clearfix"  style="min-height: 500px;">
          <div class="no-print">
            <%= render partial: 'shared/flash_messages' %>
            <%= yield :header %>
          </div>
            <div class="card">
              <%= yield %>
            </div>
        </div>
      </div>
    </section>

</body>

</html>