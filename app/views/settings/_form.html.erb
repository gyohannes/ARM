<%= simple_form_for(@setting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :exam_weight %>
    <%= f.input :interview_weight %>
    <%= f.input :additional_marks_for_female, label: 'Additional marks for female (%)' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
