  <div class="header">
      <h2>
        Settings
      </h2>
    </div>
  <div class="body table-responsive">
    <table class="table table-bordered table-striped table-hover dataTable js-exportable">
  <thead>
    <tr>
      <th>Exam weight</th>
      <th>Interview result</th>
      <th>Additional marks for female (%)</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.exam_weight %></td>
        <td><%= setting.interview_weight %></td>
        <td><%= setting.additional_marks_for_female %></td>
        <td>
          <%= link_to 'Detials', setting %> |
          <%= link_to 'Edit', edit_setting_path(setting) %> |
          <%= link_to 'Delete', setting, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
    <hr/>
<%= link_to 'New Setting', new_setting_path unless Setting.exists? %>
  </div>