<% if current_user.admin? %>
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
<div class="card">
    <div class="header">
        <h2>
            Programs
        </h2>
    </div>
    <div class="body table-responsive">
    <table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.name %></td>
        <td><%= link_to 'Edit', edit_program_path(program) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<div>
<div>
<% end %>
<br>

<%= link_to 'New Program', new_program_path, class: 'btn btn-primary' %>
