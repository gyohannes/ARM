<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-group form-float">
  <div class="form-line">
      <label class="form-label">Academic Year</label>
      <%= f.input_field :academic_year_id, collection: AcademicYear.all, class: "form-control show-tick" %>
  </div>
</div>
<div class="form-group form-float">
<div class="form-line">
    <label class="form-label">name</label>
    <%= f.input_field :name, class: "form-control show-tick" %>
</div>
</div>

<div class="form-group form-float">
<div class="form-group">
    <div class="form-line">
        <label class="form-label">Start Date</label>
        <%= f.input_field :start_time, as: :string, class: "datepicker form-control" %>
    </div>
    </div>
</div>

<div class="form-group form-float">
<div class="form-group">
    <div class="form-line">
        <label class="form-label">End Date</label>
        <%= f.input_field :end_time, as: :string, class: "datepicker form-control" %>
    </div>
    </div>
</div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
