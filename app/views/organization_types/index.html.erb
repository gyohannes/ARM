<div class="header">
  <h2>
    Organization Types
  </h2>
</div>
<div class="body table-responsive">
  <table class="table table-bordered table-striped table-hover dataTable js-exportable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_types.each do |organization_type| %>
      <tr>
        <td><%= organization_type.name %></td>
        <td><%= organization_type.description %></td>
        <td>
          <%= link_to 'Edit', edit_organization_type_path(organization_type) %>
          <%= link_to 'Delete', organization_type, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
  <hr/>
<%= link_to 'New Organization Type', new_organization_type_path %>
</div>